.fastTest{
    .TestTit{
        background: #f5f5f5;
        height: 60px;
        line-height: 60px;
        text-align: left;
        font-size:14px;
        border-bottom:#e7e7e7 1px solid;
        div.one{
            padding-left:10px;
        }
        div.two{
            padding-right:10px;
        }
        .red{
            color: #ff2626;
        }
        .yellow{
            color: #fa8013;
        }
    }
}
.detailfast{
    
    .description{
        padding:0 10px;
        width: 100%;
        color: #767676;
    }
    .selections{
        padding:0 10px;
        dl{
            
            
            margin-bottom: 10px;
            dt{
                float:left;
                border:#d9d9d9 1px solid;
                background: #eeeeee;
                height:50px;
                width:50px;
                line-height: 50px;
                font-size: 14px;
                text-align: center;
            }
            dt.wrong{
                border:#ff4d4d 1px solid;
                background: #ff4d4d;
                color: #fff;
            }
            dt.correct{
                border:#41b39c 1px solid;
                background: #41b39c;
                color: #fff;
            }
            dd{
                margin:0 0 0 49px;
                border:#d9d9d9 1px solid;
                min-height:50px;
                padding-left:5px;
                position: relative;
            }
            dd.wrong{
                border:#ff4d4d 1px solid;
                .fa{
                    color: #ff4d4d;
                    position: absolute;
                    right: 1%;
                    top:1%
                }
            }
            dd.correct{
                border:#41b39c 1px solid;
                .fa{
                    color: #41b39c;
                    position: absolute;
                    right: 1%;
                    top:1%
                }
            }
        }
    }
    .buttons{
        padding:0px 10px;
        height: auto;
        .buttonAnalysis {
             background: #41b39c;
             height: 45px;
             line-height: 45px;
        }
        .buttonPaper {
             background: #41b39c;
             line-height: 45px;
             height: 45px;
        }
    }
    .showPage {
        border: none 0 ;
    }
    .numbers{
        border:0 none;
        height: auto;
        position: relative;
        height: 40px;
        .subNumber{
            height: auto;
            border: none 0;
            z-index: 9;
            background:none;
        }
        .subNumber  span{
            border-radius: 50%;
            border:#d7d7d7 1px solid;
            display: block;
            background: #fff;
            color: #333;
            width:30px;
            height: 30px;
            line-height: 30px;
            margin: 0 auto;
        }
        .subNumber  span.desc{
            border:#41b39c 1px solid;
            color: #fff;
            background:#41b39c;
        }
        .line{
            border:#cdcdcd 1px solid;
            border-radius: 5px;
            width: 100%;
            height: 10px;
            box-shadow: 1px 2px 3px rgba(0,0,0,0.3);
            position: absolute;
            left: 0;
            top: 10px;
        }
        .linebox{
            position: absolute;
            left:0;
            top: 0;
            width: 100%;
            z-index: 10;
            background: none;
        }
    }
    
    .numbers .numberFocus{
        background: #41b39c;
        color: #fff;
        
    }
    
}
