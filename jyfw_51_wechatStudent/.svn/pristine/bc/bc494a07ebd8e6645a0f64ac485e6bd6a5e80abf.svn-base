.testIndex{
  background-color:#F5F5F5;
  height:100%;
  width: 100%;
  margin: 0 auto;
  .testindex-top{
      width:100%;
      height:15%;
      background-color:#fff;
      .text-top-left{
          text-align:right;
          padding:5% 3% 5% 0;
          color:#FE7575;
      }
      .text-top-right{
          text-align:left;
          padding:3% 5% 3% 0;
          @include font-size(14);
      }
  }
  .button-dropdown{
    &:first-child{
      margin-top: 0;
    }
    .button{
      font-weight: 300;
      font-size: 1rem;
      text-decoration: none;
      text-align: center;
      height: 100%;
      padding: 0 40px;
      margin: 0;
      display: inline-block;
      appearance: none;
      cursor: pointer;
      border: none;
      -webkit-box-sizing: border-box;
      -moz-box-sizing: border-box;
      box-sizing: border-box;
      -webkit-transition-property: all;
      transition-property: all;
      -webkit-transition-duration: .3s;
      transition-duration: .3s;
    }

    .button-primary{
      border-radius:25px;
      border:2px solid #41B39C;
      color:#41B39C;
      background: #fff;
      i{
        position: absolute;
        right: 10%;
        top: 50%;
        -webkit-transform: translateY(-50%);
        -ms-transform: translateY(-50%);
        transform: translateY(-50%);
      }
    }
    .button-dropdown-list{
      li a{
        border-bottom:1px solid #41B39C;
        color:#41B39C;
        background-color:#fff;
        font-size:0.75rem;
        text-align: center;
      }
    }
   } 
  .red{
      color:red;
  }
  .green{
      color:green;
  }

   .grayColor{
      font-size:100%!important;
      margin-top: 3rem;
      background-color: #41B39C;
      border-radius: 5px;
      margin-left:0;
   }
}
