.knowledge_heard{
    background: #41b39c;
    height: 50px;
    line-height: 50px;
    margin: 0;
    p{
        margin: 0;
        text-align: center;
        color:#fff;
        position: relative;
    }
    .fa{
        position: absolute;
        left:48%;
        top:102px;
        color: #41b39c;
    }
}
.konwledgeList{
    margin-top:20px;
    padding:0 5%;
    ul{
        padding: 0;
        margin: 0;
        li{          
            text-align: center;
            margin: 5px 0;                      
            color: #797979;
            span{
                float:right;
                color:#41b19b
            }
            div{
                margin:0 5px;
                border:#41b39c 1px solid;
                border-radius: 5px;
                background: #fff;
                padding:10px 10px;
            }
        }
    }
}
