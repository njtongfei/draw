.myserver {
    background-color: #F7F7F7;
    width: 100%;
    height: 100%;
    letter-spacing: normal;
    @include font-size(14);

    .top {
        background-color: #41B39C;
        width: 100%;
        height: 180px;

        .headpic {
            width: 80px;
            height: 80px;
            margin: 0 auto;
            background-color: #fff;
            border-radius: 50%;
        }

        .name {
            padding: 5% 0;
            text-align: center;
            vertical-align: middle;
            width: 100%;
            margin: 0 auto;
            @include font-size(18);
            color: #fff;
        }

        .schoolclass {
            text-align: center;
            vertical-align: middle;
            width: 100%;
            margin: 0 auto;
            @include font-size(13);
            color: #fff;
        }
    }

    .rounds {
        color: #fff;
        border: #0F6A59 2px solid;
        font-size: 13px;
        padding-top: 4%;
        text-align: center;
        position: absolute;
        left: 79%;
        top:31%;
        width: 70px;
        height: 70px;
        border-radius: 50%;
        background: #0F6A59;
    }

    .toptext {
        box-shadow: 1px 1px 2px #888888;
        background-color: #fff;
        padding: 9% 5%;
    }

    .contents {
        width: 100%;
        height: auto;
        padding: 3%;
        background-color: #F7F7F7;

        .upinfo {
            border-left: 2px solid #6CC3B1;
            padding: 0 0 0 2%;
            margin: 2% 0 2% 2%;

            .scoreinfo {
                background-color: #fff;
                padding: 2% 2% 2% 3%;
                box-shadow: 3px 3px 5px #E5E5E5;
                border-radius: 5px;
            }
        }
    }


    .buttons {
        width: 100%;
        margin: auto;
        height: 60px;
        margin: 0 auto;
        margin-top: 2%;
        position: fixed;
        left: 0;
        bottom: 0;
        z-index: 20;

        .subButton {
            display: inline-block;
            width: 45%;
            border-radius: 5px;
            color: white;
            font-weight: 300;
            font-size: 16px;
            font-family: "Microsoft YaHei","Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
            text-decoration: none;
            text-align: center;
            line-height: 50px;
            height: 50px;
        }

        .buttonAnswer {
            cursor: pointer;
            background: #32C0B9;
            margin-left: 0;
            float: left;
        }

        .buttonAnalysis {
            width: 45%;
            border-radius: 25px;
            cursor: pointer;
            background: #41B39C;
            margin-left: 2%;
            float: left;
        }

        .buttonPaper {
            cursor: pointer;
            background: #FF851B;
            margin-left: 2%;
            float: left;
        }

        .gaycolor {
            background-color: #B879FB;
        }
    }

    .green {
        color: #59BBA7;
    }

    .red {
        color: red;
    }
}
