.detailscore{
    background-color: #F5F5F5;
    font-family: 'Microsoft YaHei';
    width:100%;
    margin: 0 auto;
    p{
        margin: 0; padding: 0;
    }
    .detail_title{
        background-color:#fff;
        color:#000;
        font-size: 14px;
        padding: 15px;
    }
    .red{
        color: #ff1d1d;
    }
    .green{
        color: #41b29c;
    }
    .detail{
        position: relative;
        margin-top: 85px;
        font-size: 14px;
        .left{
            background: #41b29c;
            color: #fff;
            padding: 20px;
        }
        .right{
            color: #fff;
            background: #a588d7;
            padding: 20px;
        }
        .myfen{
            margin-left:-60px;
            font-family: 'Microsoft YaHei';
            font-weight:normal;
            letter-spacing:1px;
            color:#41b29c;
            border:#41b29c 2px solid;
            font-size:15px;
            padding-top:2%;
            text-align:center;
            position:absolute;
            left:50%;
            bottom:80%;
            width:120PX;
            height:120PX;
            border-radius: 50%;
            background: #FFF;
        }
    }
}


.levelall{
    padding:5% 0 5% 0;
    font-size:14px;
    text-align:center;
    height:100%;
    background-color:#F5F5F5;
    .stytest{
        padding:5% 0 5% 0;
        background-color:#fff;
    }
}

.mywrong{
    color:#fff;
    background-color:#41B29C;
    font-size:14px;
    padding:3% 0 3% 0;
    text-align:center;
}
.myerror{
    text-align:center;
    color:white;
    .errorbtn{
        height:40px;
        line-height:260%;
    }
    .btnleft{
        height:40px;
        background-color:#A588D8;
    }
    .btnright{
        height:40px;
        background-color:#41b29c;
    }
}