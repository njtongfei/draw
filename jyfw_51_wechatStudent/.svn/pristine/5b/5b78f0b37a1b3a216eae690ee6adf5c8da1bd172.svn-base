

.popBox{z-index:2000}
.popBox , .popmain , .popmask{ position:fixed; top:0; left:0; width:100%;height:100%}
.popBox .popmain{ display:table}
.popBox .popmask{ background-color:rgba(0,0,0,.5); }
.popBox .popmain .section{ display:table-cell; vertical-align:middle; text-align:center;}
.popBox .main{max-width:280px; min-width:200px; min-height:80px; border-radius:8px; background:#fff; display:inline-block; padding:15px; text-align:left}
.kd{
    background:url(/assets/images/line.gif) 0 12px repeat-x;
}
.kd span{
         background: #b879fb;
         color: #fff;
         border-radius:25px;
         padding:3px 8px;
         display: block;
         max-width: 100px;
         margin: 0 auto;
         text-align: center;
         font-size:12px;
}
.conbox{
        padding:10px;
        line-height: 26px;
        word-wrap:break-word; 
        font-size: 14px;
    }

.confirm{
       
        width:100%;
        height:35px;
         
        line-height:35px;
        background:#5291E7;
     
        border-radius: 5px;
        color: #fff;
        font-size:16px;
        text-align: center;
        margin-top:15px;
}
.scrolldiv{
    overflow: auto;
    max-height:260px;
}
.rounds {
        color: #fff;
        border: #0F6A59 2px solid;
        font-size: 13px;
        padding-top: 14%;
        text-align: center;
        width: 70px;
        height: 70px;
        border-radius: 50%;
        background: #01B391;
    }
.backgroundco{
    color:gray;
    border:2px solid gray;
    background-color:#fff;
}
.choseaccount{
    width:96%;
    .rounds{
        margin:5%;
    }
}