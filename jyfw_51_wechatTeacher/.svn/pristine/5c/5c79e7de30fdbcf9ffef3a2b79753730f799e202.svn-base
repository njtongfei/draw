.errorPage{
    width:95%;
    margin:auto;
    .showPage{
      border:1px solid #F5F5F5;
      border-radius: 7px;    
      background:#FFF;         
      min-height:10em;                              
      .blueTop{
        width:100%;
        height:0.5rem;
        border-radius: 7px 7px 0px 0px;
      }
      .content{
        .indexClass{
          display: inline-block;
        }
        .symbol{
          display: inline-block;
        }
        .description{
          display: inline-block;
        }

        
        .picture{

        }
        .selections{
          width:100%;
          margin: auto;
          .selection{
            width:50%;
            display: inline-block;
            .select{
              display: inline-block;
            }
            .symbol{
              display: inline-block;
            }
            .answer{
              display: inline-block;
            }
            .rightSymbol{
              visibility:hidden;
              display: inline-block;
              background:red;
              width:1rem;
              height:1rem;
            }
          }
        }
      }

      .showAnalysis{
        display:none;
      }
    }

    .buttons{
      width:100%;
      margin: auto;
      height:60px;
      margin-top:5%;
      .subButton{
        display: inline-block;
        width:32%;
        border-radius: 5px;
        color:white;
        font-weight: 300;
        font-size: 16px;
        font-family: "微软雅黑","Helvetica Neue Light", "Helvetica Neue", Helvetica, Arial, "Lucida Grande", sans-serif;
        text-decoration: none;
        text-align: center;
        line-height: 60px;
        height: 60px;
      }
      .buttonAnswer{
        cursor: pointer;
        background:#32C0B9;
        margin-left:0;
        float:left;
      }
      .buttonAnalysis{
        cursor: pointer;
        background:#B10DC9;
        margin-left:2%;
        float:left;
      }
      .buttonPaper{
        cursor: pointer;
        background:#FF851B;
        margin-left:2%;
        float:left;
      }
    }

    .changeItem{
      text-align:center;
      background:#FFF;
      border:1px solid #F5F5F5;
      width:100%;
      height:30px;
      border-radius: 7px;
      margin-top:5%;
      margin-bottom: 2%;
      .previous{
        width:32%;
        height:30px;
        line-height:30px;
        display:inline-block;
        cursor: pointer;
        .arrow{
          display:inline-block;
          width:30px;
          height:28px;
        }
        .textSelect{
          display:inline-block;
        }
      }
      .showOrder{
        width:32%;
        height:30px;
        line-height:30px;
        display:inline-block;
        .currentPage{
          display:inline-block;
        }
        .backslash{
          display:inline-block;
        }
        .totalPage{
          display:inline-block;
        }
      }

      .next{
        cursor: pointer;
        height:30px;
        line-height:30px;
        width:32%;
        display:inline-block;
        .arrow{
          display:inline-block;
          width:30px;
          height:28px;
        }
        .textSelect{
          display:inline-block;
        }
      }
    }
}